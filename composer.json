{
    "name": "ohlandt/required-records",
    "description": "Make sure your system has the records it depends on",
    "type": "bolt-extension",
    "keywords": [
        "records", "util", "management"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Phillipp Ohlandt",
            "email": "phillipp.ohlandt@googlemail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Ohlandt\\RequiredRecords\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\Ohlandt\\RequiredRecords\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\Ohlandt\\RequiredRecords\\RequiredRecordsExtension"
    }
}
